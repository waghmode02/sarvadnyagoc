{
  "rewrites": [
    { "source": "/(.*)", "destination": "/" }
  ],
  "installCommand": "pnpm install"
}
{
  "version": 2,
  "builds": [
    {
      "src": "vite.config.ts",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "outputDirectory": "dist",
  "cleanUrls": true
}
